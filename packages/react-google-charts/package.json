{
  "name": "react-google-charts",
  "version": "5.2.1",
  "description": "react-google-charts React component",
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.js"
  },
  "scripts": {
    "emitDeclarations": "tsc --skipLibCheck --emitDeclarationOnly",
    "build": "rollup -c & npm run emitDeclarations && npm run build-storybook",
    "start": "storybook dev -p 6006",
    "start:storybook": "storybook dev -p 6006 --ci",
    "build-storybook": "del ./storybook-static; NODE_ENV=production storybook build",
    "format:src": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "format:test": "prettier --write 'test/**/*.{js,jsx,ts,tsx}'",
    "format:sandboxes": "prettier --write 'sandboxes/**/*.{js,jsx,ts,tsx}'",
    "format:stories": "prettier --write 'stories/**/*.{js,jsx,ts,tsx}'",
    "format": "npm run format:src && npm run format:test && npm run format:sandboxes && npm run format:stories",
    "jest": "jest -c jest.config.json",
    "test:unit": "npm run jest",
    "test:size": "size-limit",
    "test": "npm run test:unit",
    "commit": "cz",
    "bumpVersion": "standard-version",
    "createGithubRelease": "simple-github-release",
    "release": "npm run bumpVersion && git push origin master --tags && npm run createGithubRelease",
    "cleanPublish": "npm run test && npm run build && rm -rf package && clean-publish --without-publish --temp-dir package",
    "updateGitHooks": "simple-git-hooks",
    "nano-staged": "nano-staged",
    "commitlint": "commitlint --edit $1",
    "chromatic": "npx chromatic"
  },
  "peerDependencies": {
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.25.7",
    "@chromatic-com/storybook": "^3.2.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@size-limit/preset-big-lib": "^11.1.6",
    "@storybook/addon-actions": "^8.4.4",
    "@storybook/addon-controls": "^8.4.4",
    "@storybook/addon-docs": "^8.4.4",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.3",
    "@swc/core": "^1.9.1",
    "@storybook/manager-api": "^8.4.4",
    "@storybook/react": "^8.4.4",
    "@storybook/react-webpack5": "^8.4.4",
    "@storybook/theming": "^8.4.4",
    "@swc/helpers": "^0.5.15",
    "@swc/jest": "^0.2.37",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.0",
    "browserslist": "^4.24.0",
    "chromatic": "^11.12.6",
    "clean-publish": "^5.0.0",
    "commitizen": "^4.3.1",
    "del-cli": "^6.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^25.0.1",
    "nano-staged": "^0.8.0",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rollup": "^4.24.0",
    "rollup-plugin-swc": "^0.2.1",
    "simple-git-hooks": "^2.11.1",
    "simple-github-release": "^1.0.0",
    "size-limit": "^11.1.6",
    "standard-version": "^9.5.0",
    "typescript": "^5.6.3",
    "storybook": "^8.4.4",
    "webpack": "^5.95.0"
  },
  "author": {
    "name": "Rakan Nimer"
  },
  "homepage": "https://react-google-charts.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RakanNimer/react-google-charts.git"
  },
  "keywords": [
    "react",
    "google",
    "charts"
  ],
  "files": [
    "dist/"
  ],
  "bugs": {
    "url": "https://github.com/RakanNimer/react-google-charts/issues"
  },
  "packageManager": "npm@10.8.3"
}
