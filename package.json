{
  "name": "react-google-charts",
  "version": "3.0.15",
  "description": "react-google-charts React component",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/index.esm.js",
  "umd": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "parcel demo/index.html -d demo-build/",
    "build": "rollup -c",
    "format:src": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "format:test": "prettier --write 'test/**/*.{js,jsx,ts,tsx}'",
    "format": "yarn format:src && yarn format:test",
    "jest": "jest -c jest.config.json",
    "test:types": "tsc --skipLibCheck --noEmit",
    "test:unit": "yarn jest",
    "test": "yarn test:types && yarn test:unit && yarn build",
    "prepublishOnly": "npm run build",
    "docz:dev": "docz dev",
    "docz:build": "docz build && mkdir docs/react-google-charts && cp -r docs/static docs/react-google-charts/static && mkdir docs/assets/ && cp -r assets/ docs/assets/",
    "docz:deploy": "yarn docz:build && cp now.json docs/ && cd docs/ && now && now alias",
    "commit": "cz",
    "release": "standard-version"
  },
  "dependencies": {
    "react-load-script": "^0.0.6"
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=16.3.0",
    "react-dom": ">=16.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^14.1.0",
    "@commitlint/config-conventional": "^14.1.0",
    "@swc/core": "^1.2.108",
    "@swc/helpers": "^0.2.14",
    "@swc/jest": "^0.2.5",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/jquery": "^3.3.5",
    "@types/react": "^17.0.34",
    "@types/react-dom": "^17.0.11",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "docz": "^0.10.2",
    "docz-theme-default": "^0.10.2",
    "jest": "^27.3.1",
    "jsdoc": "^3.4.0",
    "jsdom": "^18.1.0",
    "kill-port": "^1.3.2",
    "lint-staged": "^12.0.2",
    "object-assign": "^4.1.0",
    "parcel-bundler": "^1.12.2",
    "prettier": "^2.4.1",
    "react": "^16.9.0",
    "react-component-component": "^1.2.1",
    "react-dom": "^16.9.0",
    "rollup": "^2.60.0",
    "rollup-plugin-typescript2": "^0.31.0",
    "simple-git-hooks": "^2.7.0",
    "standard-version": "^9.3.2",
    "typescript": "^3.8.0"
  },
  "gh-pages-deploy": {
    "staticpath": "demo/dist/"
  },
  "author": "Rakan Nimer",
  "homepage": "https://rakannimer.github.io/react-google-charts/",
  "license": "MIT",
  "repository": "https://github.com/RakanNimer/react-google-charts",
  "keywords": [
    "react",
    "google",
    "charts"
  ],
  "files": [
    "dist/"
  ]
}
